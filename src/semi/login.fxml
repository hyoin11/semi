<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns:fx="http://javafx.com/fxml/1"	prefHeight="260.0" prefWidth="355.0"
	style="-fx-background-color: white;">
	<AnchorPane >
		<children>
			<Label layoutX="30.0" layoutY="50.0" text="아이디" 
			style="-fx-font-size: 18;"/>
			<TextField fx:id="logId" layoutX="120.0" layoutY="41.0" 
			prefHeight="40.0" prefWidth="195.0" style="-fx-border-color: black;" />
			<Label layoutX="30.0" layoutY="110.0" text="비밀번호" 
			style="-fx-font-size: 18;"/>
			<PasswordField fx:id="logPass" layoutX="120.0" layoutY="101.0" 
			prefHeight="40.0" prefWidth="195.0" style="-fx-border-color: black;" />
			<Button fx:id="logBtnLogin" layoutX="30.0" layoutY="170.0" prefHeight="40" prefWidth="285.0" text="로그인" 
			style="-fx-border-color: white; -fx-background-color: black; -fx-text-fill: white; -fx-font-size: 16; -fx-font-weight: bold;" />
		</children>
	</AnchorPane>
</StackPane>

