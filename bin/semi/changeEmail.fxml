<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="200.0" prefWidth="400.0"
	style="-fx-background-color: white;">
	<children>
		<Label layoutX="30.0" layoutY="40" text="이메일" 
		style="-fx-font-weight: bold; -fx-font-size: 16;" />
		<TextField fx:id="changeEmail" layoutX="110.0" layoutY="30.0" 
		prefHeight="40.0" prefWidth="250.0" style="-fx-border-color: black;" />
		<Button fx:id="changeBtnEmail" layoutX="30.0" layoutY="110.0" text="이메일 변경" prefHeight="40.0" prefWidth="330.0" 
		style="-fx-border-color: white; -fx-background-color: black; -fx-text-fill: white; -fx-font-size: 16; -fx-font-weight: bold;" />
	</children>
</AnchorPane>

